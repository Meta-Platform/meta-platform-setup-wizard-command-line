                {
                    "appType":"CLI",
                    "executable":"mytoolkit",
                    "packageNamespace":"Main.Module/Application.layer/maintenance-toolkit.cli",
                    "supervisorSocketFileName":"mytoolkit.sock"
                }
            ]
        },
        {
            "repositoryNamespace":"PlatformCoreRepo",
            "repositorySourceType": "LOCAL_FS",
